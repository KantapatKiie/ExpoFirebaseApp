{
  "expo": {
    "name": "expofirebase",
    "slug": "expofirebase",
    "version": "1.0.0",
    "orientation": "portrait",
    "privacy": "public",
    "icon": "./assets/images/logoapp.png",
    "splash": {
      "image": "./assets/images/splash-screen-wangdek.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iamcompany.expofirebase",
      "buildNumber": "1.0.0"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.iamcompany.expofirebase",
      "versionCode": 1,
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logoapp.png",
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}
